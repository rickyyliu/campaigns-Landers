    <script>
        // Geolocation script to get user's city and state
        function getLocationInfo() {
            // First try to get location using IP-based geolocation service
            fetch('https://ipapi.co/json/')
                .then(response => response.json())
                .then(data => {
                    if (data.city && data.region) {
                        document.getElementById('user-city').textContent = data.city;
                        document.getElementById('user-state').textContent = data.region;
                    }
                })
                .catch(error => {
                    // Fallback to browser geolocation if IP service fails
                    if (navigator.geolocation) {
                        navigator.geolocation.getCurrentPosition(function(position) {
                            // Use reverse geocoding service
                            fetch(`https://nominatim.openstreetmap.org/reverse?format=json&lat=${position.coords.latitude}&lon=${position.coords.longitude}`)
                                .then(response => response.json())
                                .then(data => {
                                    if (data.address) {
                                        const city = data.address.city || data.address.town || data.address.village || 'your city';
                                        const state = data.address.state || 'your state';
                                        document.getElementById('user-city').textContent = city;
                                        document.getElementById('user-state').textContent = state;
                                    }
                                });
                        });
                    }
                });
        }
        
        // Call the function when page loads
        window.onload = function() {
            getLocationInfo();
        };
    </script>
